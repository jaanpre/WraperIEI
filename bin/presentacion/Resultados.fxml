<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minHeight="400.0" minWidth="500.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentacion.ControladorListarMoviles">
   <children>
      <VBox alignment="TOP_RIGHT" prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TableView fx:id="moviles" prefHeight="334.0" prefWidth="500.0">
              <columns>
                <TableColumn fx:id="vendedor" prefWidth="75.0" text="Vendedor" />
                <TableColumn fx:id="modelo" prefWidth="75.0" text="Modelo" />
                  <TableColumn fx:id="disponibilidad" prefWidth="75.0" text="Disponibilidad" />
                  <TableColumn fx:id="precio" prefWidth="75.0" text="Precio" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button fx:id="cerrar" mnemonicParsing="false" text="Cerrar">
               <VBox.margin>
                  <Insets right="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
